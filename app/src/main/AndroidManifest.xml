<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hencoder.wzn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:hardwareAccelerated="false">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".viewDrawByBase.OnDrawActivity" />
        <!--绘制的辅助类-->
        <activity android:name=".viewDrawByBase.DrawByClipActivity" />
        <!--花呗信用的View-->
        <activity
            android:name=".viewDrawByBase.drawHuaBeiView.DrawHuaBeiActivity"
            android:hardwareAccelerated="false"
            android:screenOrientation="portrait" />
        <!--贝塞尔曲线的简单应用-->
        <activity android:name=".viewDrawByBase.pathBeiSaiEr.PathQuatActivity" />
    </application>

</manifest>